{
  "name": "@tpluscode/rdf-ns-builders",
  "version": "0.3.7",
  "description": "Generated RDF/JS namespace builders for common vocabularies",
  "main": "index.js",
  "module": "index.mjs",
  "browser": "index.mjs",
  "types": "index.d.ts",
  "scripts": {
    "generate": "ts-node -P tsconfig.build.json build/index.ts src",
    "test": "c8 --reporter=lcov mocha --require ts-node/register tests/index.ts",
    "prepack": "npm run build",
    "build": "run-p build:*",
    "build:cjs": "tsc",
    "build:esm": "babel src --out-dir . --extensions .ts --out-file-extension .mjs"
  },
  "exports": {
    "require": "./index.js",
    "import": "./index.mjs"
  },
  "files": [
    "*.js",
    "*.mjs",
    "vocabularies",
    "*.d.ts",
    "*.d.ts.map"
  ],
  "dependencies": {
    "@rdf-esm/namespace": "^0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/cli": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.10.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@rdfjs/data-model": "^1.1.2",
    "@tpluscode/eslint-config": "0.0.4",
    "@types/clownface": "^0.12.7",
    "@types/rdfjs__namespace": "^1.1.1",
    "@types/rdf-js": "^3.0.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "@types/rdf-ext": "^1.3.5",
    "@types/yargs": "^15.0.3",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "@zazuko/rdf-vocabularies": "^2020.7.13",
    "babel-plugin-add-import-extension": "^1.3.1",
    "c8": "^7.2.1",
    "chai": "^4.2.0",
    "clownface": "^0.12.1",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.1.0",
    "mocha": "^8.0.1",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "standard": "^14.3.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "yargs": "^15.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tpluscode/rdf-ns-builders.git"
  },
  "author": "Tomasz Pluskiewicz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tpluscode/rdf-ns-builders/issues"
  },
  "homepage": "https://github.com/tpluscode/rdf-ns-builders#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && tsc --noEmit"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --quiet"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
